@base <http://local/onto/myonto#> .

<http://local/onto/myonto#myonto> rdf:type owl:Ontology .

#################################################################
#    Object Properties
#################################################################

###  http://local/onto/myonto#prop1
:prop1 rdf:type owl:ObjectProperty ;
       rdfs:domain :Classe1 ;
       rdfs:range :Classe2 .


###  http://local/onto/myonto#prop2
:prop2 rdf:type owl:ObjectProperty ;
       rdfs:domain :Classe6 ;
       rdfs:range :Classe5 .


#################################################################
#    Data properties
#################################################################

###  http://local/onto/myonto#Attributo1
:Attributo1 rdf:type owl:DatatypeProperty ;
            rdfs:domain :Classe1 .


###  http://local/onto/myonto#Attributo2
:Attributo2 rdf:type owl:DatatypeProperty ;
            rdfs:domain :Classe5 .


#################################################################
#    Classes
#################################################################

###  http://local/onto/myonto#Classe1
:Classe1 rdf:type owl:Class ;
         rdfs:subClassOf [ rdf:type owl:Restriction ;
                           owl:onProperty :prop1 ;
                           owl:someValuesFrom owl:Thing
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :Attributo1 ;
                           owl:someValuesFrom rdfs:Literal
                         ] ;
         owl:disjointWith :Classe2 .


###  http://local/onto/myonto#Classe2
:Classe2 rdf:type owl:Class .


###  http://local/onto/myonto#Classe3
:Classe3 rdf:type owl:Class ;
         rdfs:subClassOf [ rdf:type owl:Class ;
                           owl:unionOf ( :Classe1
                                         :Classe2
                                       )
                         ] .


###  http://local/onto/myonto#Classe4
:Classe4 rdf:type owl:Class ;
         owl:equivalentClass [ rdf:type owl:Class ;
                               owl:oneOf ( :Individuo1
                                           :Individuo2
                                         )
                             ] .


###  http://local/onto/myonto#Classe5
:Classe5 rdf:type owl:Class ;
         rdfs:subClassOf [ rdf:type owl:Restriction ;
                           owl:onProperty [ owl:inverseOf :prop2
                                          ] ;
                           owl:someValuesFrom owl:Thing
                         ] .


###  http://local/onto/myonto#Classe6
:Classe6 rdf:type owl:Class .


###  http://local/onto/myonto#Classe7
:Classe7 rdf:type owl:Class ;
         owl:equivalentClass [ rdf:type owl:Class ;
                               owl:unionOf ( :Classe3
                                             :Classe5
                                             :Classe6
                                           )
                             ] .


#################################################################
#    Individuals
#################################################################

###  http://local/onto/myonto#Individuo1
:Individuo1 rdf:type owl:NamedIndividual ,
                     :Classe1 .


###  http://local/onto/myonto#Individuo2
:Individuo2 rdf:type owl:NamedIndividual ,
                     :Classe2 .


#################################################################
#    General axioms
#################################################################

[ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                         owl:onProperty :prop2 ;
                         owl:minCardinality "1"^^xsd:nonNegativeInteger
                       ]
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :prop2 ;
                         owl:maxCardinality "1"^^xsd:nonNegativeInteger
                       ]
                     ) ;
  rdf:type owl:Class ;
  rdfs:subClassOf :Classe6
] .


[ rdf:type owl:AllDisjointClasses ;
  owl:members ( :Classe3
                :Classe5
                :Classe6
              )
] .


