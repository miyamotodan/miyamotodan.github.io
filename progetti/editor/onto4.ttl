@prefix : <http://www.mydomain.com#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
 
:StazioneAppaltante a owl:Class ;
    rdfs:label "Stazione Appaltante" ;
    rdfs:comment "Le stazioni appaltanti sono enti che gestiscono e conducono le procedure di appalto pubblico." .
 
:ContrattoPubblico a owl:Class ;
    rdfs:label "Contratto Pubblico" ;
    rdfs:comment "I contratti pubblici sono accordi stipulati tra le stazioni appaltanti e gli operatori economici per la fornitura di lavori, servizi o forniture." .
 
:OperatoreEconomico a owl:Class ;
    rdfs:label "Operatore Economico" ;
    rdfs:comment "Gli operatori economici sono soggetti che partecipano alle gare d'appalto e possono essere selezionati per l'esecuzione di contratti pubblici." .
 
:gestisce a owl:ObjectProperty ;
    rdfs:label "gestisce" ;
    rdfs:domain :StazioneAppaltante ;
    rdfs:range :ContrattoPubblico .
 
:stipula a owl:ObjectProperty ;
    rdfs:label "stipula" ;
    rdfs:domain :StazioneAppaltante ;
    rdfs:range :ContrattoPubblico .
 
:partecipa a owl:ObjectProperty ;
    rdfs:label "partecipa" ;
    rdfs:domain :OperatoreEconomico ;
    rdfs:range :ContrattoPubblico .
 
:nomina a owl:ObjectProperty ;
    rdfs:label "nomina" ;
    rdfs:domain :StazioneAppaltante ;
    rdfs:range :ResponsabileUnicoProgetto .
 
:ResponsabileUnicoProgetto a owl:Class ;
    rdfs:label "Responsabile Unico del Progetto" ;
    rdfs:comment "Il responsabile unico del progetto (RUP) Ã¨ nominato dalle stazioni appaltanti per le fasi di programmazione, progettazione, affidamento ed esecuzione dei contratti." .